{
  "name": "revibe24",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte build --output public",
    "preview": "npm run build && wrangler pages dev .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "build-cf-types": "wrangler types && move worker-configuration.d.ts src/"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240405.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.2.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.48.0"
  },
  "type": "module",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "svelte-fast-marquee": "^0.7.0",
    "svelte-modals": "^1.3.0",
    "tailwindcss": "^3.4.3"
  }
  
}